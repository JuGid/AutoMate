{
    "name": "jugid/auto-mate",
    "description": "Automatisation par fichier Yaml",
    "type": "library",
    "require": {
        "php-webdriver/webdriver": "^1.9",
        "symfony/console": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/config": "^5.2",
        "lezhnev74/pasvl": "^0.6.4"
    },
    "authors": [
        {
            "name": "Jugid",
            "email": "gidjulien@gmail.com"
        }
    ],
    "autoload": {
     "psr-4": {
      "Automate\\": "src"
     },
     "classmap": ["src"]
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "all": [
          "@global",
          "@transformers"
        ],
        "global": [
          "vendor/bin/phpunit --colors=always --testsuite global"
        ],
        "transformers": [
          "vendor/bin/phpunit --colors=always --testsuite transformers"
        ]
      }
}
